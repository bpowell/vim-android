  let params = []
  let lastParam = {}
  call s:accept('LPAREN')
  if b:token != 'RPAREN'
    let lastParam = s:formalParameter()
    call add(params, lastParam)
    while b:token == 'COMMA' && len(lastParam.mods.flags) <= 34		" (lastParam.mods.flags & s:Flags.VARARGS) == 0
      call s:nextToken()
      let lastParam = s:formalParameter()
      call add(params, lastParam)
    endwhile
